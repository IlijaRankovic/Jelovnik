<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.example.korisnik.jelovnik">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/jelovnik"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/jelovnik"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".activities.FirstActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.SecondActivity"
            android:label="@string/activity_second">
        </activity>

        <activity
            android:name=".activities.ThirdActivity"
            android:label="@string/activity_third">
        </activity>

    </application>

</manifest>
